{
    "name": "hiblaphp/http",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Hibla\\Http\\": "src/"
        },
        "files": [
            "src/Helpers/global_functions.php",
            "src/Helpers/namespace_functions.php"
        ]
    },
    "authors": [
        {
            "name": "Reymart A. Calicdan",
            "email": "reymart.calicdan@evsu.edu.ph"
        }
    ],
    "require": {
        "hiblaphp/event-loop": "dev-main",
        "hiblaphp/promise": "dev-main",
        "hiblaphp/async": "dev-main",
        "symfony/cache": "^7.3",
        "psr/cache": "^3.0",
        "psr/simple-cache": "^3.0",
        "psr/http-message": "^2.0"
    }
    ,
      "require-dev": {
        "pestphp/pest": "^4.0",
        "phpstan/phpstan": "^2.1",
        "phpstan/phpstan-strict-rules": "^2.0",
        "laravel/pint": "^1.25"
    },
     "scripts": {
        "analyze": "./vendor/bin/phpstan analyse",
        "format": "./vendor/bin/pint",
        "format-test": "./vendor/bin/pint --test",
        "test": "./vendor/bin/pest"
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}
